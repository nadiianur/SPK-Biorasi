            <div class="d-flex justify-content-center align-items-center">
                <img src="/adminLogin.png" width="100%" height="100%" alt="Login Admin"
                    style="margin-top: 16rem; z-index: 10;">
            </div>
        </div>

        <div class="col-md-6 p-4 ps-md-0 d-flex flex-column justify-content-center" style="background: #F0FFFD;">
            <div style="padding: 12%; margin-top: 6%; z-index: 100;">
                <div class="header-text mb-5">
                    <h2
                        style="font-weight: 900 ; font-size: 32px; letter-spacing: 3.2px; text-shadow: 0 6px 10px rgba(0, 0, 0, 0.364);">
                        Selamat Datang Kembali Admin!</h2>
                </div>
                <h5>Username</h5>
                <form action="/loginAdmin" , method="post">
                    <div class="input-group mb-4" style="width: 100%;">
                        <input type="text" class="form-control form-control-lg fs-6 rounded-4"
                            style="background-color: #EBEBEB; border-radius: 20px; border: #EBEBEB;"
                            placeholder="Ketikkan username disini" id="username">
                    </div>
                    <h5>Password</h5>
                    <div class="input-group mb-1" style="width: 100%;">
                        <div class="d-flex" style="width: 500%;">
                            <input type="password" class="form-control form-control-lg fs-6 rounded-4"
                                style="background-color: #EBEBEB; border-radius: 20px; border: #EBEBEB;"
                                placeholder="Ketikkan password disini" id="password">
                            <i class="far fa-eye" id="togglePassword"
                                style="margin-left: -40px; cursor: pointer; margin-top: 16px; color: #8d9298;"></i>
                        </div>
                    </div>
                    <div class="input-group mb-3 d-flex">
                        <div class="forgot mt-2" style="margin-left: auto; margin-right: 10%;">
                            <small><a href="/forgotPass" style="text-decoration: none;">Lupa Password?</a></small>
                        </div>
                    </div>
                    <div class="input-group mb-3 justify-content-center">
                        <button class="btn btn-lg fs-6"
                            style="background-color: #FA5F00;border-radius: 30px;color: white;width: 100px; font-weight:800;"
                            type="submit">Log
                            In</button>
                    </div>
                </form>
            </div>

